  "scripts": {
    "start": "BUNDLE_TYPE=app npm run ts:scss:watch & node scripts/start.js",
    "start:https": "HTTPS=true PORT=1313 BUNDLE_TYPE=app node scripts/start.js",
    "storybook": "NODE_ENV=development STORYBOOK_ENV=development start-storybook -p 3000 -c .storybook",
    "build-storybook": "NODE_ENV=production STORYBOOK_ENV=static build-storybook -c .storybook -o .storybook_build",
    "build": "CI=true BUNDLE_TYPE=app npm run ts:scss && node scripts/build.js",
    "bundle:size": "node_modules/.bin/bundlesize --config .bundlesizerc",
    "bundle:analyzer": "[ -e ./build/stats.json ] && webpack-bundle-analyzer ./build/stats.json || echo 'webpack-bundle-analyzer: stats not available'",
    "test": "CI=true node scripts/test.js --detectOpenHandles",
    "test:noCache": "CI=true node scripts/test.js --detectOpenHandles --no-cache",
    "lint:code": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:code:fix": "npm run lint:code -- --fix",
    "lint:styles": "stylelint 'src/**/*.{css,scss}'",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "lint:fix": "npm run lint:code:fix",
    "ts:scss": "tsm 'src/**/*.module.scss' --aliasPrefixes.~ src/",
    "ts:scss:watch": "tsm -w 'src/**/*.module.scss' --aliasPrefixes.~ src/",
    "npm:clean": "rm -rf node_modules package-lock.json && npm cache clean --force && npm install -g node-sass && npm rebuild && npm install --loglevel silly",
    "prestart:https": "rm -rf ./node_modules/webpack-dev-server/ssl/server.pem && cp -f ./config/ssl/server.pem ./node_modules/webpack-dev-server/ssl",
    "postbuild": "react-snap & npm run bundle:analyzer"
  }
